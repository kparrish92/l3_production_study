	pages = {683--695},
	annote = {to read},
	file = {Antoniou et al. - 2015 - The bilingual advantage in phonetic learning.pdf:/Users/kyleparrish/Zotero/storage/4A5BVGZE/Antoniou et al. - 2015 - The bilingual advantage in phonetic learning.pdf:application/pdf},
