	number = {3},
	urldate = {2021-09-27},
	journal = {Canadian Journal of Linguistics/Revue canadienne de linguistique},
	author = {Balas, Anna},
	month = sep,
	year = {2018},
	pages = {309--338},
	file = {Full Text:/Users/kyleparrish/Zotero/storage/GX7ER62G/Balas - 2018 - English vowel perception by Polish advanced learne.pdf:application/pdf},
}

